
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="/Content/css/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/Content/css/owl/owl.carousel.min.css">
    <link rel="stylesheet" href="/Content/css/owl/owl.theme.default.min.css">
    <link rel="stylesheet" href="/Content/webfont/fontawesome/css/all.min.css">
    <link rel="stylesheet" href="/Content/css/he-fo.css">
    <link rel="stylesheet" href="/Content/css/style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
</head>

<!--بسم الله الرحمن الرحيم -->

<body>

    <!-- nav bar-->
    <nav>
        <input type="checkbox" id='check'>
        <label for="check" class="checkbtn">
            <i class="fas fa-bars"></i>
        </label>
        <label class="logo">
            <a href="">F!krah</a>
        </label>
        <ul>
            <li><a href=@Url.Action("home","Student")>Home</a></li>
            <li><a href=@Url.Action("idea","Student")>Ideas</a></li>
            <li><a href="#">Requests</a></li>
            
            <li><a href=@Url.Action("create_team","Student")>Teams</a></li>

            <li>
                <i class="fas fa-users ii"></i>
                <a href=@Url.Action("team_formation","Student")>Team Formattion</a>
            </li>
            <li>
                <i class="fas fa-user ii"></i>
                <a href=@Url.Action("profile","Student")>Profile </a>
            </li>

            <li>
                <i class="fas fa-sign-out-alt ii"></i>
                <a href=@Url.Action("log_out","Student")>Log out</a>

            </li>

            <li class="notifylist">

                <a href="#" id="show-drop-down" class="notification"><i class="fas fa-bell ii"></i><span class="badge badge-danger">2</span></a>
                <div class="your-list" id="your-list">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link active" data-toggle="tab" href="#doctor">From Doctor</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#company">From Company</a>
                        </li>

                    </ul>

                    <!-- Tab panes -->
                    @RenderSection("notify",false)
                </div>
            </li>
        </ul>
    </nav>
    @RenderSection("panner_image", false)
    


    <script src="/Content/js/jQuery3.js"></script>
    <script src="/Content/js/main.js"></script>
    <script src="/Content/owl/owl.carousel.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.0.4/popper.js"></script>
    <script src="/Content/public/css/bootstrap/js/bootstrap.min.js"></script>
    <script src="/Content/public/webfont/fontawesome/js/all.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
